{
  "name": "aurity",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev -p 9000",
    "build": "next build",
    "start": "next start -p 9000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "demo": "ts-node aurity/scripts/demo.ts",
    "demo:evidence": "ts-node aurity/scripts/export-demo-evidence.ts",
    "docker:build": "docker build -t aurity:latest -t aurity:0.1.0 .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local aurity:latest",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "next": "14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sweetalert2": "^11.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}